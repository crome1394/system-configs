sudo apt install bind9-dnsutils		# Essential DNS lookup and diagnostic tools
sudo apt install nfs-common		# NFS
sudo apt install pcscd			# PC/SC Daemon
sudo apt install mpv			# Movie Player
sudo apt install nnn			# An extremely fast, minimal file manager TUI
sudo apt install duf			# A modern, colorful disk usage viewer
sudo apt install lazpaint-qt5		# A simple paint program



